            <th scope="col">Title</th>
            <th scope="col">Price</th>
            <th scope="col">URL</th>
          </tr>
        </thead>
        <% products.forEach(product =>{ %>
            <tbody>
                <tr>
                  <th scope="row"><%=product.id%></th>
